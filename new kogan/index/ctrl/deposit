        function tixianAll(price)
        {
            $('#num').val(price);
        }
    
        $(function () {
            var countdown = 180;
            var flag = true;
            var loading = null;
    
            if ($("input[name=bank_account]").val() == '****') {
                loading = $(document).dialog({
                    type: 'notice',
                    infoIcon: '/static_new/img/loading.gif',
                    infoText: 'Loading....',
                    autoClose: 0
                });
                $(document).dialog({infoText: 'Cartão de banco não ligado, vá para a ligação!'});
                setTimeout(function () {
                    window.location.href = '/index/my/bind_bank';
                }, 2000);
            }
    
            /*验证码倒计时*/
            function time_down(obj){
                if (countdown == 0) {
                    flag = true;
                    obj.text("Enviar");
                    countdown = 180;
                    return;
                } else {
                    flag = false;
                    obj.text(countdown+"s") ;
                    countdown--;
                }
                setTimeout(function() {time_down(obj)},1000);
            }
            /*获取验证码*/
            $(".get-code").on('click',function(){
                if(check()&&flag){
                    flag = false;
    
                    var host = window.location.host;
                    if (host.startsWith('localhost')){
                        sendCode('');
                    }
                    else {
                        //grecaptcha.ready(function () {
                        //    grecaptcha.execute('6LdC99EZAAAAABfzI41Q48ZSd2Pe2aHA_vJ61go4', {action: 'submit'}).then(function (token) {
                                // Add your logic to submit to your backend server here.
                                sendCode('');//sendCode(token);
                        //    });
                        //});
                    }
                }
            })
    
            function sendCode(reCAPTCHA){
                $.ajax({
                    url:'/index/send/sendsms',
                    data: {'type':8, '_csrf_': "60436ac94bac3", 'reCAPTCHA': reCAPTCHA, 'v':3, 'num': $('#num').val()},
                    type:'POST',
                    success:function(data){
                        if(data.code==0){
                            $(document).dialog({infoText: data.info});
                            time_down($(".get-code"));
                        }else{
                            flag = true;
                            $(document).dialog({infoText: data.info});
                        }
                    },
                    error: function(err) {
                        flag = true;
                        console.log(err);
                    }
                });
            }
    
            /*检查表单*/
            function check() {
                if ($("input[name=num]").val() == '') {
                    $(document).dialog({infoText: 'please enter the withdrawal amount'});
                    return false;
                }
                if ($("input[name=mobile]").val() == '') {
                    $(document).dialog({infoText: 'please enter your mobile phone number'});
                    return false;
                }
                if ($("input[name=bank_account]").val() == '') {
                    $(document).dialog({infoText: 'please enter the bank card number'});
                    return false;
                }
                if ($("input[name=back_name]").val() == '') {
                    $(document).dialog({infoText: 'Exchange Name'});
                    return false;
                }
                if ($("input[name=paypassword]").val() == '') {
                    $(document).dialog({infoText: 'Please enter the withdrawal password'});
                    return false;
                  }
    /*            if ($("input[name=paypassword]").val() == '') {
                    $(document).dialog({infoText: '输入资金密码'});
                    return false;
                }*/
                return true;
            }
    
            /*点击提交*/
            $(".save-btn").on('click', function () {
                if (check()) {

                    var loading = null;
                    $.ajax({
                        url: '/index/ctrl/do_deposit',
                        data: $("#main-form").serialize(),
                        type: 'POST',
                        beforeSend: function () {
                            loading = $(document).dialog({
                                type: 'notice',
                                infoIcon: '/static_new/img/loading.gif',
                                infoText: 'Loading...',
                                autoClose: 0
                            });
                        },
                        success: function (data) {
                            loading.close();
                            if (data.code == 0) {
                                $(document).dialog({infoText: 'Submission is successful, please wait patiently for review.'});
                                setTimeout(function () {
                                    window.location.href = '/index/my/index';
                                }, 2000);
                            } else {
                                if (data.code == -1){
                                    $(document).dialog({infoText: data.info, autoClose: 3000});
                                    setTimeout(function () {
                                        window.location.href = '/index/ctrl/recharge';
                                    }, 3000);
                                }
                                else{
                                    $(document).dialog({infoText: data.info});
                                }
                            }
                        }
                    });
                }
            });

        })
    
    